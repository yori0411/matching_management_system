<%= form_with(model: room, local: true) do |form| %>
    <% if room.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(room.errors.count, "error") %> prohibited this room from being saved:</h2>
  
        <ul>
        <% room.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>
  
    <div class="field">
      <%= form.label :ルームID %>
      <%= form.number_field :room_id, id: :room_room_id %>
    </div>
  
  
    <div class="field">
      <%= form.label :マイID %>
      <%= form.text_field :user_id1, id: :room_user_id1 %>
    </div>
  
    <div class="field">
      <%= form.label :パートナーID %>
      <%= form.text_field :user_id2, id: :room_user_id2 %>
    </div>
  
  
    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
  