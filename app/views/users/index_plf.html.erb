<p id="notice"><%= notice %></p>

<h1>プロフィール確認画面</h1>

<table>
  <thead>
    <tr>
      <th>名前</th>
      <th>性別</th>
      <th>プロフィール画像</th>
      <th>q1</th>
      <th>q2</th>
      <th>q3</th>
      <th>q4</th>
      <th>q5</th>
      <th>q6</th>
      <th>q7</th>
      <th>q8</th>
      <th>q9</th>
      <th>q10</th>
      <th>q11</th>
      <th>q12</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.gender %></td>
        <td><%= user.filename %></td>
        <td><%= user.q1 %></td>
        <td><%= user.q2 %></td>
        <td><%= user.q3 %></td>
        <td><%= user.q4 %></td>
        <td><%= user.q5 %></td>
        <td><%= user.q6 %></td>
        <td><%= user.q7 %></td>
        <td><%= user.q8 %></td>
        <td><%= user.q9 %></td>
        <td><%= user.q10 %></td>
        <td><%= user.q11 %></td>
        <td><%= user.q12 %></td>
        <td><%= link_to '詳細', user %></td>
        <td><%= link_to '編集', edit_user_path(user) %></td>
        <td><%= link_to '削除', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'プロフィール登録画面', new_user_path %>
<%= link_to 'ログアウト', :auth_logout, method: :delete %>
